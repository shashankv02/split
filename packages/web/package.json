{
  "name": "@split/web",
  "version": "1.0.0",
  "main": "src/index.ts",
  "license": "MIT",
  "scripts": {
    "start": "parcel src/index.html --port 6969",
    "build": "rm -rf dist && NODE_ENV=production parcel build src/index.html",
    "test": "jest --passWithNoTests",
    "generate": "graphql-codegen",
    "typecheck": "tsc --noEmit",
    "lint": "eslint --cache --ext .jsx,.js,.ts,.tsx src",
    "prettify": "prettier --write './src/**/*.{ts,tsx}'",
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,json,ts,tsx,html}": [
      "prettier --write",
      "git add"
    ],
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix --cache --ext .jsx,.js,.ts,.tsx src",
      "git add"
    ]
  },
  "dependencies": {
    "@apollo/react-hooks": "^3.0.0",
    "@graphql-codegen/cli": "1.13.4",
    "@graphql-codegen/typescript": "1.13.4",
    "@graphql-codegen/typescript-operations": "1.13.4",
    "@graphql-codegen/typescript-react-apollo": "1.13.4",
    "@noquarter/hooks": "^1.1.6",
    "@noquarter/ui": "^2.0.8",
    "@noquarter/utils": "^1.0.5",
    "@reach/router": "^1.2.1",
    "apollo-boost": "^0.4.4",
    "axios": "^0.19.0",
    "dayjs": "^1.8.11",
    "graphql": "^14.1.1",
    "lodash.throttle": "^4.1.1",
    "logrocket": "^1.0.0",
    "logrocket-react": "^3.0.0",
    "polished": "^3.0.4",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-dropzone": "^10.0.5",
    "react-error-boundary": "^1.2.3",
    "react-media": "^1.9.2",
    "styled-media-query": "^2.1.2",
    "unfetch": "^4.1.0",
    "use-media": "^1.3.0"
  },
  "devDependencies": {
    "@testing-library/react": "10.0.4",
    "@types/graphql": "14.2.3",
    "@types/jest": "24.9.1",
    "@types/lodash.throttle": "4.1.6",
    "@types/node": "11.15.12",
    "@types/reach__router": "1.3.5",
    "@types/react": "16.9.34",
    "@types/react-dom": "16.9.7",
    "@types/react-dropzone": "4.2.2",
    "@types/styled-components": "4.4.3",
    "@typescript-eslint/eslint-plugin": "2.30.0",
    "@typescript-eslint/parser": "2.30.0",
    "babel-eslint": "10.1.0",
    "eslint": "6.8.0",
    "eslint-config-prettier": "6.11.0",
    "eslint-config-react-app": "5.2.1",
    "eslint-plugin-flowtype": "4.7.0",
    "eslint-plugin-import": "2.20.2",
    "eslint-plugin-jsx-a11y": "6.2.3",
    "eslint-plugin-react": "7.19.0",
    "eslint-plugin-react-hooks": "2.5.1",
    "jest": "24.9.0",
    "jest-dom": "3.5.0",
    "parcel-bundler": "1.12.4",
    "prettier": "1.19.1",
    "ts-jest": "24.3.0",
    "typescript": "3.8.3"
  }
}
